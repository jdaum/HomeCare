<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.whjt2_000.homecare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EnterInformation"
            android:label="@string/title_activity_enter_information"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.whjt2_000.homecare.MainActivity" />
        </activity>
        <activity
            android:name=".StockAnswerSetUp"
            android:label="@string/title_activity_set_up"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.whjt2_000.homecare.MainActivity" />
        </activity>
        <activity
            android:name=".ReviewInformation"
            android:label="@string/title_activity_review_information"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.whjt2_000.homecare.MainActivity" />
        </activity>
        <activity
            android:name=".ReivewDate"
            android:label="@string/title_activity_reivew_date"
            android:parentActivityName=".ReviewInformation"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.whjt2_000.homecare.ReviewInformation" />
        </activity>
        <activity
            android:name=".ReviewDate"
            android:label="@string/title_activity_review_date"
            android:parentActivityName=".ReviewInformation"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.whjt2_000.homecare.ReviewInformation" />
        </activity>
    </application>

</manifest>
